class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        
        stack=[]
        check_breackets={"}":"{" , "]":"[" , ")":"("}

        for char in s:
            if char in "({[":
                stack.append(char)
            elif char in "]})":
                if stack and stack[-1]==check_breackets[char]:
                    stack.pop()
                else:
                    return False 


        return not stack 
