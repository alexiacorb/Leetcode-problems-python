class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """

        # use a stack, if the stack is empty at the end it means they were in order
        stack=[]
        check_breackets={"}":"{" , "]":"[" , ")":"("}

        for char in s:
            if char in "({[":
                stack.append(char)
            elif char in "]})":
                if stack and stack[-1]==check_breackets[char]:
                    #stack checks if the stack is not empty  
                    #stack[-1] -Â»means the last position in the stack
                    stack.pop()
                else:
                    return False 


        return not stack 
